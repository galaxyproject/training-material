        ]
      },
      "subtopic": "muon-spectroscopy",
      "priority": 2,
      "abbreviations": {
        "AIRSS": "Ab Initio Random Structure Search",
        "DFT": "Density Functional Theory",
        "UEP": "Unperturbed Electrostatic Potential"
      },
